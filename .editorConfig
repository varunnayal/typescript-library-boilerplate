root = true

[*]
charset = utf-8

end_of_line = lf
insert_final_newline = true

indent_style = space
indent_size = 4
tab_width = 4

trim_trailing_whitespace = true

[*.json]
indent_style = tab
indent_size = tab
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
max_line_length = off
